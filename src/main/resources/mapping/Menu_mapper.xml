<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xueyou.ssm.dao.MenuDao">
    <select id="getMenuList" statementType="STATEMENT" parameterType="Map" resultType="Map">
        SELECT * from menu where STATUS = 1 ORDER BY level ASC
    </select>
</mapper>